{
  "schemaVersion": 1,
  "id": "customendermanbehavior",
  "version": "${version}",
  "name": "CustomEndermanBehavior",
  "description": "Allow you to disable some behavior for the enderman entity (like picking up blocks, teleporting, etc.)",
  "authors": [
    "Skyfy16"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/customendermanbehavior/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "ch.skyfy.customendermanbehavior.CustomEndermanBehaviorMod"
      }
    ]
  },
  "mixins": [
    "customendermanbehavior.mixins.json"
  ],
  "depends": {
    "java": ">=17",
    "minecraft": "1.19.4",
    "fabricloader": ">=0.14.17",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.9.2+kotlin.1.8.10"
  }
}
